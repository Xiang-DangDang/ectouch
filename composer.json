{
    "name": "ectouch/ectouch",
    "type": "project",
    "description": "A Modern E-Commerce Platform.",
    "keywords": [
        "ectouch",
        "framework",
        "laravel"
    ],
    "homepage": "https://www.ectouch.cn",
    "license": "GPL-3.0",
    "support": {
        "issues": "https://github.com/ectouch/ectouch/issues?state=open",
        "forum": "https://www.ectouch.cn/forum/",
        "wiki": "https://www.ectouch.cn/wiki/",
        "source": "https://github.com/ectouch/ectouch"
    },
    "require": {
        "php": "^7.1.3",
        "aliyuncs/oss-sdk-php": "^2.3",
        "endroid/qr-code": "^3.5",
        "firebase/php-jwt": "^5.0",
        "fideloper/proxy": "^4.0",
        "jialeo/payment": "^0.9.1",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "nesbot/carbon": "^2.11",
        "overtrue/easy-sms": "^1.1",
        "overtrue/pinyin": "^4.0",
        "overtrue/wechat": "^4.1",
        "phpoffice/phpspreadsheet": "^1.6",
        "ramsey/uuid": "^3.8",
        "workerman/phpsocket.io": "^1.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/constant.php",
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
